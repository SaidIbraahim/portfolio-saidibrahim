{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "dist" }
    }
  ],
  "routes": [
    {
      "src": "/preview-test",
      "dest": "/index.html"
    },
    {
      "src": "/",
      "status": 301,
      "headers": { "Location": "https://portfolio.saidibrahim.tech" },
      "has": [
        {
          "type": "host",
          "value": "saidibrahim.tech"
        }
      ]
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
